  },
  "homepage": "https://github.com/lcrespom/fz2",
  "devDependencies": {
    "browserify": "^12.0.1",
    "exorcist": "^0.4.0",
    "http-server": "^0.8.5",
    "tsify": "^0.12.2",
    "typescript": "^1.6.2",
    "watchify": "^3.6.1"
  }
